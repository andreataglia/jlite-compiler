}

class Vector {
  Point p1;
  Point p2;

  Int norm() {
    Int res;
    res = this.square(p1.x - p2.x) + (p2.y - p1.y) * (p2.y - p1.y);
    res = sqrt(res);
    return res + 67 - 67;
  }

  Int square(Int a){
    return a * a;
  }

  Vector getPoint() {
    return this;
  }

  Int sqrt(Int a) {
    Int r;
     r= 0;
    while(r * r < a)
   {

   r = r+1;
   }
   return r;
  }
}